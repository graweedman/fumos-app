// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}


model fumos {
  id String @id @default(uuid())
  name String
  power Int?
}

model users {
  id String @id @default(uuid())
  createdAt DateTime @default(now())
  email String @unique
  username String?
}
